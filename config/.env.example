# DevOps Pipeline Orchestrator Environment Configuration

# Kafka settings
KAFKA_HOST=localhost
KAFKA_PORT=9092

# GitHub webhook settings
GITHUB_WEBHOOK_SECRET=your_github_webhook_secret_here

# Pipeline configuration
PIPELINE_CONFIG_PATH=config/pipeline.yml

# Notification settings
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your/slack/webhook/url
MAX_RETRIES=3
RETRY_DELAY_SECONDS=60

# Logging configuration
LOG_LEVEL=INFO

# Gemini API settings for Security Agent
GEMINI_API_KEY=your_gemini_api_key_here
# Optional: override the default Gemini API URL
# GEMINI_API_URL=https://generativelanguage.googleapis.com/v1beta/models/gemini-2.5-flash:generateContent

# Mistral AI settings for pipeline AI summary
MISTRAL_API_KEY=your_mistral_api_key_here
MISTRAL_MODEL=mistral-tiny
# Optional: override the default Mistral API URL
# MISTRAL_API_URL=https://api.mistral.ai/v1/chat/completions